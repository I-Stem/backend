# App's running environment
NODE_ENV=

# App's running port
PORT=


# Max request body size
APP_MAX_UPLOAD_LIMIT=

# Max Parameter Limit that are allowed in 
# URL-encoded data
APP_MAX_PARAMETER_LIMIT=

# Is CORS Enabled
# default: true (Accepts only boolean)
CORS_ENABLED=true

# API Prefix
API_PREFIX=

# JSON Web Tokem Expiry time
# Note: In minutes
JWT_EXPIRES_IN=

# Enable Queue Monitor Server
QUEUE_HTTP_ENABLED=true

# QUEUE Monitor's HTTP Port
# Note: This value is required iff QUEUE_HTTP_ENABLED is true
QUEUE_HTTP_PORT=5550


# REDIS QUEUE Settings
REDIS_QUEUE_PORT=6379
REDIS_QUEUE_URL='127.0.0.1'
REDIS_QUEUE_DB=
REDIS_QUEUE_PREFIX='q'
REDIS_QUEUE_PASSWORD=

# App's global details
APP_NAME=
APP_DESCRIPTION=
APP_URL=
APP_KEYWORDS="web-server, typescript-express, typescript, express"
COMPANY_NAME="I-Stem"

# App's secret key
# Note: This value will be used in CSRF Token 
# as well as in JSON Web Token (ie. JWT)
APP_SECRET=

# Mongoose uri with the database name
MONGOOSE_URL=


# AWS's key and secret
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=
AWS_BUCKET_NAME="storage"
EMAIL_FROM_ADDRESS=



# Service api

SERVICE_API_HOST="https://istem.com"

# Credit information
USER_ONBOARDING_CREDITS=
CREDIT_EXPIRY=
GOOGLE_ID="hello"
GOOGLE_SECRET="world"
EXEMPT_ENDPOINT="/api/auth,/api/afc"
PORTAL_URL="http://test.server.com"

DIAGNOSTIC_EMAIL_REPORTING_ENABLED = "true"
DIAGNOSTIC_EMAIL='john@example.com'

CUSTOMER_FEEDBACK_REPORTING_ENABLED = 'true'
CUSTOMER_FEEDBACK_RECEIVING_EMAIL = 'john@example.com'
ESCALATION_EMAIL='john@example.com'
JOB_APPLICATION_RECEIVING_EMAIL='john@example.com'


